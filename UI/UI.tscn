[sub_resource type="GDScript" id="GDScript_byuxr"]
resource_name = "PostGameView"
script/source = "extends CenterContainer

@export var label_time: Label
@export var label_distance: Label
@export var label_boxes: Label
@export var label_bottles: Label



# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	visibility_changed.connect(_set_data)

func _set_data():
	label_time.text =  \"TOTAL GAME TIME: \" + str(int(floor(GameSignals.game_total_time))) + \" sec\"
	label_distance.text =  \"TOTAL DISTANCE RAN: \" + str(int(floor(GameSignals.game_total_distance))) + \"m\"
	label_boxes.text =  \"BOXES HIT: \" + str(GameSignals.boxes_hit_counter)
	label_bottles.text =  \"BLOOD COLLECTED: \" + str(GameSignals.bottles_collected_counter)
	
"

