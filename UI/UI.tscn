[sub_resource type="GDScript" id="GDScript_f1fl4"]
resource_name = "blood_bar"
script/source = "extends GridContainer


@export var blood_pieces: Array[Control]


func _ready() -> void:
	GameSignals.health_changed.connect(calc_view)



func calc_view(cur_health: float):
	var a = cur_health / 100 * blood_pieces.size()
	
	for i in range(blood_pieces.size()):
		if a > i and blood_pieces[i].visible == false:
			blood_pieces[i].visible = true
		if a < i and blood_pieces[i].visible == true:
			blood_pieces[i].visible = false
	

	
"

